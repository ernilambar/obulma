{
	"copyFiles": [
		{
			"from": [
				"**",
				"!*.md",
				"!*.json",
				"!*.lock",
				"!*.yaml",
				"!*.js",
				"!src/**/*",
				"!sass/**/*",
				"!scripts/**/*",
				"!node_modules/**/*",
				"!deploy/**/*"
			],
			"to": "deploy/temp/"
		}
	],
	"copyFilesSettings": {
		"ignoreDotFilesAndFolders": true
	}
}
