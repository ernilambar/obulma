{
  "name": "obulma",
  "version": "1.0.7",
  "description": "Simple theme based on Bulma.",
  "repository": "github:ernilambar/obulma",
  "author": "Nilambar Sharma",
  "license": "MIT",
  "scripts": {
    "dev": "gulp",
    "build": "gulp build",
    "version": "grunt version",
    "eslint": "eslint \"scripts/**/*.{js,jsx}\" --quiet",
    "eslint:fix": "eslint \"scripts/**/*.{js,jsx}\" --quiet --fix",
    "format": "prettier --write \"sass/**/*.scss\"",
    "pot": "wpi18n makepot --domain-path=languages --exclude=vendor,deploy,node_modules",
    "textdomain": "wpi18n addtextdomain --exclude=vendor,deploy,node_modules",
    "deploy": "shx rm -rf deploy/ && shx mkdir deploy && copy-files-from-to --config .copyfiles.json && cd deploy/ && shx mv temp $npm_package_name && bestzip ../$npm_package_name.zip * && cd .. && shx mv $npm_package_name.zip deploy/"
  },
  "prettier": "@wordpress/prettier-config",
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "devDependencies": {
    "@wordpress/browserslist-config": "^4.1.2",
    "@wordpress/eslint-plugin": "^12.6.0",
    "@wordpress/prettier-config": "^1.4.0",
    "bestzip": "^2.2.1",
    "browser-sync": "^2.27.10",
    "bulma": "^0.9.4",
    "copy-files-from-to": "^3.2.2",
    "del": "^6.1.1",
    "eslint": "^8.19.0",
    "grunt": "^1.5.3",
    "grunt-replace": "^2.0.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-uglify": "^3.0.2",
    "node-wp-i18n": "^1.2.6",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "shx": "^0.3.4"
  }
}